{
  "version": "0.1.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Express Server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/server/index.js",
      "restart": true,
      "runtimeExecutable": "node",
      "console": "integratedTerminal",
      "watch": true,
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
