  "scripts": {
    "prepare": "cd .. && husky install client/.husky",
    "start": "git add .husky/pre-commit && vite --host",
    "dev": "vite",
    "dev-build": "vite build",
    "deploy-build": "npm run ts-check && npm run test && npm version patch --no-git-tag-version && vite build",
    "preview": "vite preview --host",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --runInBand --coverage",
    "ts-check": "tsc --noEmit",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,scss,css}\"",
    "safe-check": "./scripts/safe-check.sh",
    "pre-commit-hook": "npm run safe-check && npm run lint:fix && npm run prettier:fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
